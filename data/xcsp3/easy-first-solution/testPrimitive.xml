<instance format="XCSP3" type="CSP">
  <variables>
    <var id="x"> 0..5 </var>
    <var id="z"> 0..5 </var>
    <var id="z"> 0..5 </var>
  </variables>
  <constraints>
    <intension> le(x,4) </intension>
    <group>
      <intension> gt(%0,%1) </intension>
      <args> x z </args>
      <args> z z </args>
    </group>
    <intension> ne(x,z) </intension>
    <intension> eq(x,add(z,z)) </intension>
  </constraints>
</instance>
